      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>LikeSkuFcstLR=if(position([prod].[itm])==position([pror].[itmr]),FcstEditFilt,0)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>calc_LikeSkuFcstRL</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>LikeSkuFcstRL=if(AnalogSku,if(FlagB,LikeSkuFcstR*AnalogKoef,if(LevelStrGroupI==0,LikeSkuFcstR.level([loc].[rgn])*AnalogKoef*SlsWb.level([prod].[cat])/SlsWb.level([prod].[cat]+[loc].[rgn]),AnalogKoef*FcstProrLocsU/CntAnalogSkuNAbcStrU.level([locs].top))),0)</value>
      </attribute>
    </data_model>
  </data_model>
