<?xml version="1.0" encoding="UTF-8"?>
<data_set type="com.retek.ride.rule.newmodel.RuleGroup">
  <attribute name="name">
    <value>XYZ_calc</value>
  </attribute>
  <attribute name="description">
    <value>a rule group</value>
  </attribute>
  <attribute name="role">
    <value>user</value>
  </attribute>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>clc_XYZLastSalesDateIntC</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>XYZLastSalesDateIntClnd=indexlast([clnd].top,lsover&gt;0)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>calc_XYZLastSalesDateInt</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>XYZLastSalesDateInt=XYZLastSalesDateIntClnd.ambig_pop</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>calc_XYZSqrDiffSlsAvgSls</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>XYZSqrDiffSlsAvgSls=(lsover-tssum(lsover,XYZLastSalesDateInt-XYZWindowLength+1,XYZLastSalesDateInt)/XYZWindowLength)*(lsover-tssum(lsover,XYZLastSalesDateInt-XYZWindowLength+1,XYZLastSalesDateInt)/XYZWindowLength)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>calc_XYZSigma</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>XYZSigma=sqrt(tssum(XYZSqrDiffSlsAvgSls,XYZLastSalesDateInt-XYZWindowLength+1,XYZLastSalesDateInt)/XYZWindowLength)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>calc_XYZKoeff</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>XYZKoeff=sqrt(tssum(XYZSqrDiffSlsAvgSls,XYZLastSalesDateInt-XYZWindowLength+1,XYZLastSalesDateInt)/XYZWindowLength)/(tssum(lsover,XYZLastSalesDateInt-XYZWindowLength+1,XYZLastSalesDateInt)/XYZWindowLength)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>calc_XYZGroup</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>XYZGroup=if(XYZKoeff&lt;XYZThresXY,"X",if(XYZKoeff&lt;XYZThresYZ,"Y","Z"))</value>
      </attribute>
    </data_model>
  </data_model>
</data_set>
